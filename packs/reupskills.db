{
    "_id": "1xO2uybqMcduS1qU",
    "name": "Submersible Vehicle Repair",
    "type": "skill",
    "img": "modules/starwarsd6-essentialcompanion-od6s/art/items/skillIcons/tec_subvehiclerepair.svg",
